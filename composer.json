{
    "require-dev": {
        "pestphp/pest": "^3.8",
        "nunomaduro/phpinsights": "^2.13"
    },
    "scripts": {
        "all": [
            "@lint",
            "@test"
        ],
        "lint": "vendor/bin/phpinsights",
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "ramsey/uuid": "^4.7",
        "slim/slim": "^4.14"
    }
}
